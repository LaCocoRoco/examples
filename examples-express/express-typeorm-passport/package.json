  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev src/index.ts",
    "start": "NODE_ENV=production node build/index.js",
    "build": "npm run lint && rimraf build && tsc",
    "lint": "eslint src",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -f lib/typeorm/ormconfig.ts",
    "typeorm:migration:gen": "npm run typeorm migration:generate -- -n migration",
    "typeorm:migration:run": "npm run typeorm migration:run"
  },
  "devDependencies": {
    "@types/config": "^0.0.40",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.9",
    "@types/passport": "^1.0.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-github": "^1.1.7",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typeorm": "^0.2.44",
    "winston": "^3.6.0"
  }
}
