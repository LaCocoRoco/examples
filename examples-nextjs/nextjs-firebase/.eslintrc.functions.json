{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["prettier"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [{ "files": ["*.ts"] }],
  "rules": {
    "no-unused-vars": "error"
  }
}
